{
  "env": {
    "browser": true,
    "es2021": true,
    "env.allowLocalModels" = false;
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-restricted-globals": ["error", "event", "fdescribe"]
  },
  "globals": {
    "self": "readonly"
  }
}
